<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="cz.inited.$safeprojectname$" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" defaultlocale="en-US">
  <name>$projectname$</name>
  <description>A blank project for Cordova projects from Inited solutions targeted to Android, iOS and Windows</description>
  <author href="http://www.inited.cz" email="info@inited.cz">Inited Solutions</author>
  <content src="index.html" />
  <access origin="*" />
  <vs:features />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="10.0" />
  <!-- Support for Cordova 5.0.0 plugin system -->
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <platform name="android">
    <icon src="resources/icons/icon-36-ldpi.png" density="ldpi" />
    <icon src="resources/icons/icon-48-mdpi.png" density="mdpi" />
    <icon src="resources/icons/icon-72-hdpi.png" density="hdpi" />
    <icon src="resources/icons/icon-96-xhdpi.png" density="xhdpi" />
  </platform>
  <platform name="ios">
    <!-- iOS 8.0+ -->
    <!-- iPhone 6 Plus  -->
    <icon src="resources/icons/icon-60-3x.png" width="180" height="180" />
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch  -->
    <icon src="resources/icons/icon-60.png" width="60" height="60" />
    <icon src="resources/icons/icon-60-2x.png" width="120" height="120" />
    <!-- iPad -->
    <icon src="resources/icons/icon-76.png" width="76" height="76" />
    <icon src="resources/icons/icon-76-2x.png" width="152" height="152" />
    <!-- iOS 6.1 -->
    <!-- Spotlight Icon -->
    <icon src="resources/icons/icon-40.png" width="40" height="40" />
    <icon src="resources/icons/icon-40-2x.png" width="80" height="80" />
    <!-- iPhone / iPod Touch -->
    <icon src="resources/icons/icon-57.png" width="57" height="57" />
    <icon src="resources/icons/icon-57-2x.png" width="114" height="114" />
    <!-- iPad -->
    <icon src="resources/icons/icon-72.png" width="72" height="72" />
    <icon src="resources/icons/icon-72-2x.png" width="144" height="144" />
    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="resources/icons/icon-small.png" width="29" height="29" />
    <icon src="resources/icons/icon-small-2x.png" width="58" height="58" />
    <!-- iPad Spotlight and Settings Icon -->
    <icon src="resources/icons/icon-50.png" width="50" height="50" />
    <icon src="resources/icons/icon-50-2x.png" width="100" height="100" />
  </platform>
  <platform name="windows">
    <icon src="resources/icons/Square150x150Logo.scale-100.png" width="150" height="150" />
    <icon src="resources/icons/Square150x150Logo.scale-240.png" width="360" height="360" />
    <icon src="resources/icons/Square30x30Logo.scale-100.png" width="30" height="30" />
    <icon src="resources/icons/Square310x310Logo.scale-100.png" width="310" height="310" />
    <icon src="resources/icons/Square44x44Logo.scale-240.png" width="106" height="106" />
    <icon src="resources/icons/Square70x70Logo.scale-100.png" width="70" height="70" />
    <icon src="resources/icons/Square71x71Logo.scale-240.png" width="170" height="170" />
    <icon src="resources/icons/StoreLogo.scale-100.png" width="50" height="50" />
    <icon src="resources/icons/StoreLogo.scale-240.png" width="120" height="120" />
    <icon src="resources/icons/Wide310x150Logo.scale-100.png" width="310" height="150" />
    <icon src="resources/icons/Wide310x150Logo.scale-240.png" width="744" height="360" />
  </platform>
  <platform name="android">
    <splash src="resources/splash/screen-hdpi-landscape.png" density="land-hdpi" />
    <splash src="resources/splash/screen-ldpi-landscape.png" density="land-ldpi" />
    <splash src="resources/splash/screen-mdpi-landscape.png" density="land-mdpi" />
    <splash src="resources/splash/screen-xhdpi-landscape.png" density="land-xhdpi" />
    <splash src="resources/splash/screen-hdpi-portrait.png" density="port-hdpi" />
    <splash src="resources/splash/screen-ldpi-portrait.png" density="port-ldpi" />
    <splash src="resources/splash/screen-mdpi-portrait.png" density="port-mdpi" />
    <splash src="resources/splash/screen-xhdpi-portrait.png" density="port-xhdpi" />
  </platform>
  <platform name="ios">
    <splash src="resources/splash/screen-iphone-portrait.png" width="320" height="480" />
    <splash src="resources/splash/screen-iphone-portrait-2x.png" width="640" height="960" />
    <splash src="resources/splash/screen-ipad-portrait.png" width="768" height="1024" />
    <splash src="resources/splash/screen-ipad-portrait-2x.png" width="1536" height="2048" />
    <splash src="resources/splash/screen-ipad-landscape.png" width="1024" height="768" />
    <splash src="resources/splash/screen-ipad-landscape-2x.png" width="2048" height="1536" />
    <splash src="resources/splash/screen-iphone-568h-2x.png" width="640" height="1136" />
    <splash src="resources/splash/screen-iphone-portrait-667h.png" width="750" height="1334" />
    <splash src="resources/splash/screen-iphone-portrait-736h.png" width="1242" height="2208" />
    <splash src="resources/splash/screen-iphone-landscape-736h.png" width="2208" height="1242" />
  </platform>
  <platform name="windows">
    <splash src="resources/splash/SplashScreen.scale-100.png" width="620" height="300" />
    <splash src="resources/splash/SplashScreen.scale-240.png" width="1152" height="1920" />
    <splash src="resources/splash/SplashScreenPhone.scale-240.png" width="1152" height="1920" />
  </platform>
</widget>